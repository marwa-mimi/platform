{% extends "/layout/layout.twig" %}{% block title %} {{parent()}} - Liste des messages{% endblock title %}{% block content %}    <div class="row-fluid">        <div class="span12">            <div class="row-fluid">                <div class="span9 well" style="min-height:302px">                <ul class="nav nav-pills border">                    <li class="pull-left"><h4>Liste des Messages</h4></li>                    <li class="pull-right"></li>                </ul>                {% if msgs == NULL%}                        <div class="alert alert-info"><p class="text-center"><strong>Vous n'avez aucun message.</strong></p></div>                {%else%}                     {%for msg in msgs %}                        <ul class="unstyled">                            <li class="well" style="background-color: rgb(248, 248, 248);min-height: 30px;">                                <p class="pull-left">                                    <span><i class="icon-user"></i> <strong>{{msg.login}}</strong></span>                                </p>                                <a class="pull-right btn" href="/message/msgclientcomm/voirmsgcommclient/{{msg.idcom}}/{{msg.idp}}/{{msg.login}}"><i class="icon-eye-open"></i> voir message</a>                            </li>                        </ul>                     {%endfor%}                 {%endif%}            </div>                <div class="span3">                      {% block slider2 %}{{parent()}}{% endblock slider2 %}                 </div>            </div>        </div>       </div>    {% block partenaires %} {{parent()}} {% endblock partenaires %}    {% endblock content %}